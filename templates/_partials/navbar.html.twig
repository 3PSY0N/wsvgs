                    <span class="ml-2 font-bold">Categories</span>
                    <span class="stroke-slate-300 text-sm transition duration-300" :class="{'rotate-180': menuDropdown}">
                        <svg fill="none" viewBox="0 0 24 24" stroke-width="1.5" class="h-6 w-6">
                          <path stroke-linecap="round" stroke-linejoin="round" d="M19.5 8.25l-7.5 7.5-7.5-7.5"/>
                        </svg>
                    </span>
                </div>
            </div>
            <ul class="mx-auto mt-2 w-4/5 text-sm" x-show="menuDropdown" x-transition>
                {% for i in 0..25 %}
                    <li>
                        <a href="{{ path('app_home') }}" class="block p-2.5 mt-3 cursor-pointer border-l-2 border-l-transparent hover:border-l-sky-300 transition duration-300">
                            <span class="ml-2 font-semibold">Home</span>
                        </a>
                    </li>
                {% endfor %}
            </ul>
            <a href="{{ path('app_home') }}" class="block p-2.5 mt-3 cursor-pointer border-l-2 border-l-transparent hover:border-l-sky-300 transition duration-300">
